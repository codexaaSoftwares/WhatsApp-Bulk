VITE_APP_NAME="Photo Studio Management"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="production"

# Use relative URL for same-domain setup (admin.lvclicks.in/api)
# Or use full URL for separate subdomain (https://api.lvclicks.in)
VITE_API_BASE_URL=/admin/
VITE_API_TIMEOUT=30000

VITE_AUTH_TOKEN_KEY="admin_auth_token"
VITE_AUTH_REFRESH_TOKEN_KEY="admin_refresh_token"
VITE_AUTH_TOKEN_EXPIRY="3600000"

VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false

VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true

